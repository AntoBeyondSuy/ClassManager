<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.muc.domain.Notification" table="notification">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>

		<property name="first_link" column="first_link"></property>
		<property name="name" column="name"></property>
		<property name="first_words" column="first_words"></property>
		<property name="second_link" column="second_link"></property>
		<property name="second_words" column="second_words"></property>
		<property name="time" column="time"></property>
		<property name="receiver_id" column="receiver_id"></property>

		</class>
</hibernate-mapping>





